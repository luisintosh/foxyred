<p id="notice"><%= notice %></p>

<h1>Ads</h1>

<table>
  <thead>
    <tr>
      <th>Default</th>
      <th>Position</th>
      <th>Network</th>
      <th>Code</th>
      <th>Type</th>
      <th>Price</th>
      <th>Traffic source</th>
      <th>Countries</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ads.each do |ad| %>
      <tr>
        <td><%= ad.default %></td>
        <td><%= ad.position %></td>
        <td><%= ad.network %></td>
        <td><%= ad.code %></td>
        <td><%= ad.type %></td>
        <td><%= ad.price %></td>
        <td><%= ad.traffic_source %></td>
        <td><%= ad.countries %></td>
        <td><%= ad.start_date %></td>
        <td><%= ad.end_date %></td>
        <td><%= ad.weight %></td>
        <td><%= link_to 'Show', ad %></td>
        <td><%= link_to 'Edit', edit_ad_path(ad) %></td>
        <td><%= link_to 'Destroy', ad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ad', new_ad_path %>
